<p id="notice"><%= notice %></p>

<h1>Message Subjects</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Messageble</th>
      <th>User</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @message_subjects.each do |message_subject| %>
      <tr>
        <td><%= message_subject.title %></td>
        <td><%= message_subject.messageble %></td>
        <td><%= message_subject.user %></td>
        <td><%= message_subject.organization %></td>
        <td><%= link_to 'Show', message_subject %></td>
        <td><%= link_to 'Edit', edit_message_subject_path(message_subject) %></td>
        <td><%= link_to 'Destroy', message_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Message Subject', new_message_subject_path %>
